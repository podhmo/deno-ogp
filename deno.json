{
  "name": "@podhmo/ogp",
  "version": "0.1.0",
  "license": "MIT",
  "compilerOptions": {
    "lib": [
      "dom",
      "deno.ns"
    ]
  },
  "exports": {
    ".": "./src/mod.ts"
  },
  "includes": [
    "src/**/*.ts",
    "LICENSE",
    "deno.json",
    "deno.lock",
    "README.md"
  ],
  "tasks": {
    "ok": "deno check src/**/*.ts && deno test && deno fmt --check && deno lint src",
    "run:example": "deno run -A ./exmaples/collect-ogp.ts https://github.com/podhmo/deno-ogp",
    "build": "esbuild --bundle --tsconfig=deno.json --outfile=dist/index.mjs --sourcemap mod.ts"
  }
}
